{
  "name": "node-db4-project",
  "version": "1.0.0",
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "latest": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run latest && npm run seed",
    "cleanup": "knex seed:run --specific=00-cleanup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BloomInstituteOfTechnology/node-db4-project.git"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.14",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.15"
  }
}
